package chap09.EX2;

class AccountInfo {
	public int account;
	public double balance;
	
	
	public AccountInfo(int account, double balance) {
		super();
		this.account = account;
		this.balance = balance;
	}


	public void accountPrint() {
		account = 1234567890;
		balance = 100000.0;
		System.out.printf("계좌번호: %s\n 잔액: %s\n", this.account, this.balance);
	}
	
	
	
	class Bank{
		public double input;
		public double output;
		
		public Bank(double input, double output) {
			super();
			this.input = input;
			this.output = output;
		}
		public void printInput() {
			System.out.printf("%s 원이 예금되었습니다.\n",this.input);
			}
		
		public void printOutput() {
			
			System.out.printf("%s 원이 인출되었습니다.\n",this.output);
			
		}
	}
	
//	public boolean checkAccount(int inputAccount) {
//		return this.account.equals(inputAccount);
//	}
	
}
