package objectMethod;

public class Person { //extends object
	String name;
	int age;
	
	
	public Person(String name, int age) {
		super(); //Object();
		this.name = name;
		this.age = age;
	}


	@Override
	public String toString() {
		return "Person [name은 " + name + ", age는" + age + "]";
		//Person [강효경= 강효경]
	}
	
	
}
